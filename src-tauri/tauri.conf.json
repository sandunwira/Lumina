{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Lumina",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true
      },
      "fs": {
        "all": false,
        "exists": true,
        "readDir": true,
        "readFile": true,
        "writeFile": true
      }
    },
    "bundle": {
      "active": true,
      "publisher": "KoolApps Team",
      "copyright": "KoolApps Team",
      "category": "Entertainment",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.koolapps.lumina",
      "longDescription": "",
      "shortDescription": "",
      "externalBin": [],
      "resources": [],
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "en-US"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false,
      "dialog": true,
      "endpoints": [

      ],
      "windows": {
        "installMode": "basicUi"
      },
      "pubkey": ""
    },
    "windows": [
      {
        "fullscreen": true,
        "title": "Lumina",
        "decorations": false,
        "resizable": false
      }
    ]
  }
}